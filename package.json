{
  "name": "eslint-config-outfunnel",
  "version": "1.0.3",
  "description": "Javascript style guide",
  "main": "index.js",
  "engines": {
    "node": ">= 6"
  },
  "scripts": {
    "test": "echo \\\"Error: no test specified\\\" && exit 1",
    "lint": "$(npm bin)/eslint --report-unused-disable-directives .",
    "ncu": "$(npm bin)/ncu"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/outfunnel/eslint-config-outfunnel.git"
  },
  "author": "Outfunnel",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/outfunnel/eslint-config-outfunnel/issues"
  },
  "homepage": "https://github.com/outfunnel/eslint-config-outfunnel#readme",
  "dependencies": {
    "confusing-browser-globals": "^1.0.8"
  },
  "peerDependencies": {
    "eslint": "^5.16.0 || ^6.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0"
  },
  "devDependencies": {
    "eslint": "^6.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "husky": "^3.0.5",
    "lint-staged": "^9.2.5",
    "npm-check-updates": "^3.1.21"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run ncu",
      "post-merge": "npm install"
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run lint",
      "git add"
    ]
  }
}
